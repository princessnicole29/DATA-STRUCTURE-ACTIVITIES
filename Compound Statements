#include <iostream>
using namespace std;

int main()
{
    int grade;
    cout<< "Enter your grade: ";
    cin>> grade;
    
    if(grade >= 75){
        cout<< "CONGRATULATIONS!!"<<endl;
        cout<<"KEEP UP THE GOOD WORK!"<<endl;
    }
        else{
            cout<<"Result: FAIL!!"<<endl;
        }
    
    return 0;
}
