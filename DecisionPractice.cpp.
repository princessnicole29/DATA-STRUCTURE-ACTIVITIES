  #include <iostream>
  using namespace std;

  int main() {
     int grade, course;
     
     cout << "Enter your grade (0 -100): ";
     cin>> grade;
     cout << "Enter your course code (1-3): ";
     cin>> course;
     
     if (grade >= 75 && grade <= 100) {
         cout << "Result: Pass"<< endl;
     }
     else{
         cout << "Result: Fail"<< endl;
     }
     
     switch (course)    {
         case 1: 
         cout << "Course: NETCOM1"<< endl;
         break;
         
         case 2:
         cout<< "Course: Data Structures"<< endl;
         break;
         
         case 3:
         cout<< "Course: Information Management"<< endl;
         break;
     }
     
     
      return 0;
  }
  
